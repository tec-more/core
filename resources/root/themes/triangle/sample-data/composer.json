{
    "name": "sgsoft-studio/tuke",
    "description": "The CMS based on Laravel Framework 5.4",
    "authors": [
        {
            "name": "Tedozi Manson",
            "email": "duyphan.developer@gmail.com"
        }
    ],
    "keywords": [
        "framework",
        "laravel",
        "tuke",
        "laravel tuke",
        "laravel website editor",
        "laravel cms"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "sgsoft-studio/base": "3.1.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/",
            "Tuke\\Themes\\Triangle\\": "themes/triangle/src",
            "Tuke\\Plugins\\ContactForm\\": "plugins/contact-form/src",
            "Tuke\\Plugins\\Captcha\\": "plugins/captcha/src"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}